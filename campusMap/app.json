{
  "expo": {
    "name": "campusMap",
    "slug": "campusMap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo2.png", 
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8EFE0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo2.png", 
        "backgroundColor": "#F8EFE0"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.najatzaidane.campusMap"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "2b8a9211-49ce-4c3a-b037-6e9ed21fb07a"
      }
    }
  }
}
